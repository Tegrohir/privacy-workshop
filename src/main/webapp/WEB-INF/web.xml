<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	<display-name>Privacy Workshop</display-name>
	<servlet>
		<servlet-name>StartServlet</servlet-name>
		<servlet-class>nl.hu.bscs.privacygame.view.StartServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>nl.hu.bscs.privacygame.view.LoginServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>QuestionOneServlet</servlet-name>
		<servlet-class>nl.hu.bscs.privacygame.view.QuestionOneServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>QuestionTwoServlet</servlet-name>
		<servlet-class>nl.hu.bscs.privacygame.view.QuestionTwoServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>AdminServlet</servlet-name>
		<servlet-class>nl.hu.bscs.privacygame.view.AdminServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>StartServlet</servlet-name>
		<url-pattern>/start</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>QuestionOneServlet</servlet-name>
		<url-pattern>/question1</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>QuestionTwoServlet</servlet-name>
		<url-pattern>/question2</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>AdminServlet</servlet-name>
		<url-pattern>/admin</url-pattern>
	</servlet-mapping>
</web-app>
